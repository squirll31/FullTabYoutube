{
  "manifest_version": 2,

  "name": "Popout test",
  "description": "Pop out the current youtube video or playlist",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}