{
  "manifest_version": 2,

  "name": "Popout test",
  "description": "Pop out the current youtube video or playlist",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "background" : {
    "scripts" : ["popup.js"],
    "persistent" : false
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}